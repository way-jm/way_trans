.history{
  top:45px;
  position: absolute;
  bottom: 0;
  width: 100%;
}
.container{
  padding: 10px 8px;
  box-sizing: border-box;
}
.item{
  display: block;
  margin-bottom: 8px;
}
.right{
  text-align: right;
}
.right .content{
  background: #E4E7ED;
  color:#303133
}
.content{
  background: #07c160;
  display: inline-block;
  border-radius: 5px;
  max-width: 75%;
  padding: 5px 8px;
  color:white;
  font-size: 17px;
  text-align: left;
}
.target{
  font-weight: bold;
}

